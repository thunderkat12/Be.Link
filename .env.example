# Vercel Configuration
# Copie este arquivo para .env.local e preencha com seus valores

# Vercel Project Settings (obtenha no painel da Vercel)
VERCEL_TOKEN=your_vercel_token_here
VERCEL_ORG_ID=your_organization_id_here  
VERCEL_PROJECT_ID=your_project_id_here

# Environment Variables (opcional)
NODE_ENV=production
VITE_APP_NAME=BeLink
VITE_APP_VERSION=1.0.0

# Analytics (opcional - para monitoramento)
VITE_ANALYTICS_ID=your_analytics_id_here

# Custom Domain (opcional)
CUSTOM_DOMAIN=your-domain.com